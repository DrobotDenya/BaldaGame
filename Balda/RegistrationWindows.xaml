<Window x:Class="Balda.RegistrationWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RegistrationWindows" Height="300" Width="300">
    <Grid Background="#FFAEB52C">
        <StackPanel HorizontalAlignment="Left" Height="195" Margin="77,10,0,0" VerticalAlignment="Top" Width="150">
            <Label Content="First Name"/>
            <TextBox Height="23" TextWrapping="Wrap" Text=""/>
            <Label Content="Second Name"/>
            <TextBox Height="23" TextWrapping="Wrap" Text=""/>
            <Label Content="Password"/>
            <TextBox Height="23" TextWrapping="Wrap" Text=""/>
            <Label Content="ConfirmPassword"/>
            <TextBox Height="23" TextWrapping="Wrap" Text=""/>
        </StackPanel>
        <Button Content="Register" HorizontalAlignment="Left" Margin="77,230,0,0" VerticalAlignment="Top" Width="150" RenderTransformOrigin="0.5,0.5" Foreground="#FF1CF112">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-8.751"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
    </Grid>
</Window>
